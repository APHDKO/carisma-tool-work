<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="carisma.xutils.regulatory.importer.superior.ui.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="RCP Perspective"
            class="carisma.xutils.regulatory.importer.superior.ui.Perspective"
            id="carisma.xutils.regulatory.importer.superior.ui.perspective">
      </perspective>
   </extension>
   <extension
      point="org.eclipse.ui.views">
      <view
      		category="carisma.ui.eclipse"
            class="carisma.xutils.regulatory.importer.superior.ui.SettingsView"
            id="carisma.xutils.regulatory.importer.superior.ui.SettingsView"
            icon="platform:/plugin/carisma.ui.eclipse/icons/Logo16x16.png"
            name="Settings">
      </view>
      <view
      		category="carisma.ui.eclipse"
            class="carisma.xutils.regulatory.importer.superior.ui.LogView"
            id="carisma.xutils.regulatory.importer.superior.ui.LogView"
            icon="platform:/plugin/carisma.ui.eclipse/icons/Logo16x16.png"
            name="Log">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="carisma.perspective">
         <view
               id="carisma.xutils.regulatory.importer.superior.ui.SettingsView"
               relationship="stack"
               relative="carisma.ui.eclipse.views.AnalysisResultsView">
         </view>
         <view
               id="carisma.xutils.regulatory.importer.superior.ui.LogView"
               relationship="stack"
               relative="carisma.ui.eclipse.views.AnalysisResultsView">
         </view>
      </perspectiveExtension>
   </extension>

</plugin>
